{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "pnpm --filter shared build && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "auth:generate": "better-auth generate --config ./src/auth.ts --output ./src/db/auth-schema.ts --yes",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@better-auth/expo": "^1.4.18",
    "better-auth": "^1.4.18",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "pg": "^8.13.1",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.18",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
