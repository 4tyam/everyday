{
	"expo": {
		"name": "everyday",
		"slug": "everyday",
		"scheme": "everyday",
		"version": "1.0.0",
		"orientation": "portrait",
		"userInterfaceStyle": "automatic",
		"web": {
			"bundler": "metro"
		},
		"plugins": [
			"expo-router",
			[
				"expo-image-picker",
				{
					"cameraPermission": "Allow everyday to access your camera to save memories."
				}
			]
		],
		"android": {
			"package": "com.atyam.everyday"
		},
		"ios": {
			"bundleIdentifier": "com.atyam.everyday"
		}
	}
}
